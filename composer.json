{
    "name": "phower/router",
    "description": "PHP routing package compliant with PSR-7.",
    "type": "library",
    "require": {
        "php": "^5.6 || ^7.0",
        "psr/http-message": "^1.0",
        "phower/arrays": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "squizlabs/php_codesniffer": "^2.7",
        "symfony/var-dumper": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Phower\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhowerTest\\Router\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pedro Ferreira",
            "email": "pedro@phower.com"
        }
    ]
}
